{
  "general": {
    "random_seed": 0,
    "split_style": "cluster",
    "validation_percent": 0.15,
    "feature_representation": "morganfingerprint-2-1024"
  },
  "datasets":{
    "bace":{
      "suffix": ".csv",
      "X": "mol",
      "y": "pIC50",
      "y_process": "x > 6",
      "y_new": "active"
    },
    "clintox": {
      "suffix": ".csv.gz",
      "X": "smiles",
      "y": "CT_TOX"
    },
    "deepchem_Lipophilicity": {
      "suffix": ".csv",
      "short_name": "lipophilicity",
      "X": "smiles",
      "y": "exp",
      "y-process": "x >= -0.4 and x <= 5.6",
      "y_new": "drug_like"
    },
    "HIV": {
      "suffix": ".csv",
      "X": "smiles",
      "y": "HIV_active"
    },
    "Solubility_delaney-processed": {
      "suffix": ".csv",
      "short_name": "sol_del",
      "X": "smiles",
      "y": "measured log solubility in mols per litre",
      "y_process": "x >= -1",
      "y_new": "binned_sol"
    },
    "tox21": {
      "suffix": ".csv.gz",
      "X": "smiles",
      "y": "NR-AhR"
    }
  },
  "models": {
    "SVC": {
      "param_grid": [
        {
          "kernel": ["linear"],
          "C": [0.0001, 0.001, 0.1, 1, 10, 100, 1000]
        },
        {
          "kernel": ["poly"],
          "degree": [2, 3, 4],
          "C": [0.0001, 0.001, 0.1, 1, 10, 100, 1000]
        },
        {
          "kernel": ["rbf"],
          "gamma": ["auto", "scale"],
          "C": [0.0001, 0.001, 0.1, 1, 10, 100, 1000]
        }
      ],
      "best_params": {}
    }
  }
}
